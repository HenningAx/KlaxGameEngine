<local:BaseInspectorControl x:Class="KlaxEditor.UserControls.InspectorControls.TypeInspector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KlaxEditor.UserControls.InspectorControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
            DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <ComboBox Margin="0, 2, 0, 2" Style="{StaticResource ComboBoxStyle}" Name="AssetSelector" SelectionChanged="Selector_OnSelectionChanged" HorizontalAlignment="Stretch" ItemTemplate="{StaticResource TypeSelectorEntry}">
    </ComboBox>
</local:BaseInspectorControl>
