<local:BaseInspectorControl x:Class="KlaxEditor.UserControls.InspectorControls.BoolInspector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KlaxEditor.UserControls.InspectorControls"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid HorizontalAlignment="Left" Margin="0, 3, 0, 3">
        <DockPanel>
            <CheckBox Name="Checkbox" Checked="Checkbox_Checked" Unchecked="Checkbox_Checked" DockPanel.Dock="Right" HorizontalAlignment="Right" />
        </DockPanel>
    </Grid>
</local:BaseInspectorControl>
